{
  "name": "ibrahim/azure-queue",
  "description": "Laravel 7 Queue Driver for Microsoft Azure Storage Queue",
  "keywords": [
    "laravel",
    "azure",
    "storage",
    "queue",
    "microsoft"
  ],
  "license": "MIT",
  "authors": [{
    "name": "Ibrahim Rahmani",
    "email": "ibrahim@xala.no"
  }],
  "require": {
    "php": "^7.3",
    "illuminate/queue": ">=5.8,<7.11",
    "microsoft/azure-storage-queue": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.5",
    "orchestra/testbench": "3.8.*",
    "mockery/mockery": "~1.3.1",
    "php-coveralls/php-coveralls": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "AzureQueueLaravel\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "extra": {
    "laravel": {
      "providers": [
        "AzureQueueLaravel\\AzureQueueServiceProvider"
      ]
    }
  }
}